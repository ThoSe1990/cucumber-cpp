@PACKAGE_INIT@

set(CWTCUCUMBER_VERSION PACKAGE_LIBRARY_VERSION)

set_and_check(CWTCUCUMBER_INCLUDE_DIR "@PACKAGE_INCLUDE_INSTALL_DIR@")
set_and_check(CWTCUCUMBER_LIBRARY_DIR "@PACKAGE_LIBRARY_INSTALL_DIR@")

check_required_components(cwtcucumber)

include(CMakeFindDependencyMacro)

if(NOT TARGET cwt::cucumber)
    if(UNIX)
        include(${CMAKE_CURRENT_LIST_DIR}/cwtcucumberTargetsLinux.cmake)
    else()
        include(${CMAKE_CURRENT_LIST_DIR}/cwtcucumberTargetsWindows.cmake)
    endif()
endif()
